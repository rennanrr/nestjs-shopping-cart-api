{
   "name": "api-cart",
   "version": "0.0.1",
   "description": "Exercise API using Nestjs and Typeorm",
   "author": "Rennan Ribas",
   "license": "MIT",
   "scripts": {
      "prebuild": "rimraf dist",
      "build": "nest build",
      "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
      "start": "ts-node src/index.ts",
      "start:prod": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
      "start:dev": "nodemon --config nodemon.json",
      "start:dev:db": "./src/scripts/start-db.sh",
      "start:dev:db:seed:cart": "ts-node -r tsconfig-paths/register src/scripts/seedShoppingCart.ts",
      "start:dev:db:seed:product": "ts-node -r tsconfig-paths/register src/scripts/seedProduct.ts",
      "start:debug": "nest start --debug --watch",
      "lint": "tslint -p tsconfig.json -c tslint.json",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "pretypeorm": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/write-typeorm-config.ts",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "typeorm:migration:generate": "npm run typeorm -- migration:create -n",
      "typeorm:migration:run": "npm run typeorm -- migration:run",
      "nest": "nest"
   },
   "dependencies": {
      "@nestjs/common": "^6.7.2",
      "@nestjs/core": "^6.7.2",
      "@nestjs/platform-express": "^6.7.2",
      "@nestjs/swagger": "^3.1.0",
      "@nestjs/typeorm": "^6.2.0",
      "class-transformer": "^0.2.3",
      "class-validator": "^0.10.2",
      "dotenv": "^8.1.0",
      "pg": "^7.12.1",
      "reflect-metadata": "^0.1.10",
      "rimraf": "^3.0.0",
      "rxjs": "^6.5.3",
      "swagger-ui-express": "^4.1.2",
      "typeorm": "0.2.32",
      "mysql": "^2.14.1"
   },
   "devDependencies": {
      "@nestjs/cli": "^6.9.0",
      "@nestjs/schematics": "^6.7.0",
      "@nestjs/testing": "^6.7.1",
      "@types/express": "^4.17.1",
      "@types/jest": "^24.0.18",
      "@types/lodash": "^4.14.142",
      "@types/node": "^8.0.29",
      "@types/supertest": "^2.0.8",
      "jest": "^24.9.0",
      "nodemon": "^1.19.3",
      "prettier": "^1.18.2",
      "supertest": "^4.0.2",
      "ts-jest": "^24.1.0",
      "ts-loader": "^6.1.1",
      "ts-node": "3.3.0",
      "tsconfig-paths": "^3.9.0",
      "tslint": "^5.20.0",
      "typescript": "3.3.3333"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "./coverage",
      "testEnvironment": "node"
   }
}
